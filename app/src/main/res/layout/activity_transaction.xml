<?xml version="1.0" encoding="utf-8"?>
<!-- Auto layout, variables, and unit scale are not yet supported -->
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/create_wallet"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clipToOutline="true"
    android:background="#FAF0FF"
    android:orientation="vertical">
    <!-- Auto layout, variables, and unit scale are not yet supported -->
    <RelativeLayout
        android:id="@+id/rectangle_1"
        android:layout_width="match_parent"
        android:layout_height="65dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="0dp"
        android:layout_marginRight="0dp"
        android:gravity="center_vertical">

        <TextView
            android:id="@+id/new_trans"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/add_new_trans"
            android:textAppearance="@style/select_wall"
            android:layout_centerInParent="true"
            />
    </RelativeLayout>

    <TextView
        android:id="@+id/amount"
        android:layout_width="82dp"
        android:layout_height="24dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:gravity="top"
        android:lineSpacingExtra="8sp"
        android:text="@string/amount"
        android:textAppearance="@style/account_nam"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="93dp"
        android:textColor="#292929"/>

    <View
        android:id="@+id/rectangle_2"
        android:layout_width="328dp"
        android:layout_height="47dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:background="@drawable/wide_field_white"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="117dp" />


    <LinearLayout
        android:id="@+id/rectangle_3"
        android:layout_width="328dp"
        android:layout_height="47dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:background="@drawable/wide_picker_white"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:paddingHorizontal="10dp"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="185dp">

        <TextView
            android:id="@+id/category"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/category"
            android:textAppearance="?android:attr/textAppearanceSmall"
            tools:layout_editor_absoluteX="51dp"
            tools:layout_editor_absoluteY="199dp" />

        <ImageView
            android:id="@+id/category_picker"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:contentDescription="@string/icon_description"
            android:src="@drawable/ic_keyboard_arrow_down_24"
            tools:layout_editor_absoluteX="335dp"
            tools:layout_editor_absoluteY="196dp" />
    </LinearLayout>

    <View
        android:id="@+id/rectangle_4"
        android:layout_width="328dp"
        android:layout_height="47dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:background="@drawable/wide_field_white"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="281dp" />

    <TextView
        android:id="@+id/note"
        android:layout_width="82dp"
        android:layout_height="24dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:gravity="top"
        android:lineSpacingExtra="8sp"
        android:text="@string/notes"
        android:textAppearance="@style/account_nam"
        android:textColor="#292929"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="257dp" />

    <LinearLayout
        android:id="@+id/rectangle_5"
        android:layout_width="328dp"
        android:layout_height="47dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:background="@drawable/wide_picker_white"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:paddingHorizontal="10dp"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="355dp">

        <TextView
            android:id="@+id/date"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/date"
            android:textAppearance="?android:attr/textAppearanceSmall"
            tools:layout_editor_absoluteX="51dp"
            tools:layout_editor_absoluteY="199dp" />

        <ImageView
            android:id="@+id/date_picker"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_keyboard_arrow_down_24"
            tools:layout_editor_absoluteX="335dp"
            tools:layout_editor_absoluteY="196dp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/rectangle_6"
        android:layout_width="328dp"
        android:layout_height="47dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:background="@drawable/wide_picker_white"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:paddingHorizontal="10dp"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="418dp">

        <TextView
            android:id="@+id/wallet"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/wallet"
            android:textAppearance="?android:attr/textAppearanceSmall"
            tools:layout_editor_absoluteX="51dp"
            tools:layout_editor_absoluteY="199dp" />

        <ImageView
            android:id="@+id/wallet_picker"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_keyboard_arrow_down_24"
            tools:layout_editor_absoluteX="335dp"
            tools:layout_editor_absoluteY="196dp" />
    </LinearLayout>

    <RelativeLayout
        android:id="@+id/button"
        android:layout_width="240dp"
        android:layout_height="34dp"
        android:background="@drawable/lanjut_btn_canvas"
        android:gravity="center_horizontal"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="573dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:lineSpacingExtra="0sp"
            android:text="@string/save_trans"
            android:textAppearance="@style/button"
            android:textColor="@color/white" />
    </RelativeLayout>


    <!-- Auto layout, variables, and unit scale are not yet supported -->


</androidx.constraintlayout.widget.ConstraintLayout>