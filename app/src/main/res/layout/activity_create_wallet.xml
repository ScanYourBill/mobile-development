<?xml version="1.0" encoding="utf-8"?>
<!-- Auto layout, variables, and unit scale are not yet supported -->
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/create_wallet"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clipToOutline="true"
    android:background="#FAF0FF"
    android:orientation="vertical">
    <!-- Auto layout, variables, and unit scale are not yet supported -->
    <RelativeLayout
        android:id="@+id/rectangle_1"
        android:layout_width="match_parent"
        android:layout_height="65dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="0dp"
        android:layout_marginRight="0dp"
        android:background="#FFFFFF"
        android:gravity="center_vertical">

        <TextView
            android:id="@+id/new_wall"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/add_new_wal"
            android:textAppearance="@style/select_wall"
            android:layout_centerInParent="true"
            />
    </RelativeLayout>

    <TextView
        android:id="@+id/account_nam"
        android:layout_width="82dp"
        android:layout_height="24dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:gravity="top"
        android:lineSpacingExtra="8sp"
        android:text="@string/account_nam"
        android:textAppearance="@style/account_nam"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="93dp"
        android:textColor="#292929"/>

    <View
        android:id="@+id/rectangle_2"
        android:layout_width="328dp"
        android:layout_height="47dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:background="@drawable/wide_field_white"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="117dp" />

    <View
        android:id="@+id/rectangle_3"
        android:layout_width="328dp"
        android:layout_height="47dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:background="@drawable/wide_field_white"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="205dp" />

    <TextView
        android:id="@+id/amount"
        android:layout_width="82dp"
        android:layout_height="24dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:gravity="top"
        android:lineSpacingExtra="8sp"
        android:text="@string/account_nam"
        android:textAppearance="@style/account_nam"
        android:textColor="#292929"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="181dp" />

    <View
        android:id="@+id/rectangle_4"
        android:layout_width="328dp"
        android:layout_height="47dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:background="@drawable/wide_field_white"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="295dp" />

    <TextView
        android:id="@+id/note"
        android:layout_width="82dp"
        android:layout_height="24dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:gravity="top"
        android:lineSpacingExtra="8sp"
        android:text="@string/account_nam"
        android:textAppearance="@style/account_nam"
        android:textColor="#292929"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="271dp" />

    <View
        android:id="@+id/rectangle_5"
        android:layout_width="328dp"
        android:layout_height="153dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:background="@drawable/wide_field_white_big"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="390dp" />

    <TextView
        android:id="@+id/icon_pick"
        android:layout_width="82dp"
        android:layout_height="24dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:gravity="top"
        android:lineSpacingExtra="8sp"
        android:text="@string/account_nam"
        android:textAppearance="@style/account_nam"
        android:textColor="#292929"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="366dp" />

    <RelativeLayout
        android:id="@+id/button"
        android:layout_width="240dp"
        android:layout_height="34dp"
        android:background="@drawable/lanjut_btn_canvas"
        android:gravity="center_horizontal"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="573dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:lineSpacingExtra="0sp"
            android:text="@string/save_wall"
            android:textAppearance="@style/button"
            android:textColor="@color/white" />
    </RelativeLayout>


    <!-- Auto layout, variables, and unit scale are not yet supported -->


</androidx.constraintlayout.widget.ConstraintLayout>