<?xml version="1.0" encoding="utf-8"?>
<!-- Auto layout, variables, and unit scale are not yet supported -->
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/create_wallet"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:clipToOutline="true"
    android:background="#FAF0FF"
    android:orientation="vertical">
    <!-- Auto layout, variables, and unit scale are not yet supported -->
    <RelativeLayout
        android:id="@+id/rectangle_1"
        android:layout_width="match_parent"
        android:layout_height="65dp"
        android:layout_marginLeft="0dp"
        android:layout_marginTop="0dp"
        android:layout_marginRight="0dp"
        android:gravity="center_vertical">

        <TextView
            android:id="@+id/search_title"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="@string/search"
            android:textAppearance="@style/select_wall"
            android:layout_centerInParent="true"
            />
    </RelativeLayout>


    <View
        android:id="@+id/zoom_dynami"
        android:layout_width="159dp"
        android:layout_height="157dp"
        android:background="@drawable/search_img_big"
        tools:layout_editor_absoluteX="126dp"
        tools:layout_editor_absoluteY="87dp" />

    <LinearLayout
        android:id="@+id/rectangle_6"
        android:layout_width="328dp"
        android:layout_height="47dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:background="@drawable/wide_picker_white"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:paddingHorizontal="10dp"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="441dp">

        <TextView
            android:id="@+id/wallet"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/category"
            android:textAppearance="?android:attr/textAppearanceSmall"
            tools:layout_editor_absoluteX="51dp"
            tools:layout_editor_absoluteY="199dp" />

        <ImageView
            android:id="@+id/wallet_picker"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_keyboard_arrow_down_24"
            tools:layout_editor_absoluteX="335dp"
            tools:layout_editor_absoluteY="196dp" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/rectangle_3"
        android:layout_width="328dp"
        android:layout_height="47dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:background="@drawable/wide_picker_white"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:paddingHorizontal="10dp"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="311dp">

        <TextView
            android:id="@+id/category"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/search"
            android:textAppearance="?android:attr/textAppearanceSmall"
            tools:layout_editor_absoluteX="51dp"
            tools:layout_editor_absoluteY="199dp" />

    </LinearLayout>

    <LinearLayout
        android:id="@+id/rectangle_5"
        android:layout_width="328dp"
        android:layout_height="47dp"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:background="@drawable/wide_picker_white"
        android:gravity="center_vertical"
        android:orientation="horizontal"
        android:paddingHorizontal="10dp"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="378dp">

        <TextView
            android:id="@+id/date"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_weight="1"
            android:text="@string/act_type"
            android:textAppearance="?android:attr/textAppearanceSmall"
            tools:layout_editor_absoluteX="51dp"
            tools:layout_editor_absoluteY="199dp" />

        <ImageView
            android:id="@+id/date_picker"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:src="@drawable/ic_keyboard_arrow_down_24"
            tools:layout_editor_absoluteX="335dp"
            tools:layout_editor_absoluteY="196dp" />
    </LinearLayout>

    <!-- Auto layout, variables, and unit scale are not yet supported -->
    <RelativeLayout
        android:id="@+id/clear_btn"
        android:layout_width="96dp"
        android:layout_height="46dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/grey_btn"
        tools:layout_editor_absoluteX="41dp"
        tools:layout_editor_absoluteY="559dp">
        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:lineSpacingExtra="0sp"
            android:text="@string/clear"
            android:textAppearance="@style/button"
            android:textColor="@color/white" />
    </RelativeLayout>

    <RelativeLayout
        android:id="@+id/search_btn"
        android:layout_width="96dp"
        android:layout_height="46dp"
        android:layout_alignParentLeft="true"
        android:layout_alignParentTop="true"
        android:background="@drawable/purple_btn_canvas"
        tools:layout_editor_absoluteX="273dp"
        tools:layout_editor_absoluteY="559dp">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:lineSpacingExtra="0sp"
            android:text="@string/search"
            android:textAppearance="@style/button"
            android:textColor="@color/white" />
    </RelativeLayout>


</androidx.constraintlayout.widget.ConstraintLayout>